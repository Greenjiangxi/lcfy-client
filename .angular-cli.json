{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "lcfy-client"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "tsconfig": "tsconfig.app.json",
      "prefix": "app",
      "styles": [
        "styles.scss",
        "theme.scss"
      ],
      "environmentSource": "env/env.ts",
      "environments": {
        "dev": "env/env.ts",
        "prod": "env/env.prod.ts"
      },
      "scripts": []
    }
  ],
  "defaults": {
    "styleExt": "scss",
    "component": {}
  }
}
